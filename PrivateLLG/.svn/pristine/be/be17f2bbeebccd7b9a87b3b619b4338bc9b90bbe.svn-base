package com.llg.privateproject.entities;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.List;


public class ProdInfoAppBean implements Serializable{

	private static final long serialVersionUID = -6654618973028520655L;
	private String name ;//名称
	private BigDecimal price ;//价格
	private BigDecimal originalPrice ;//原始价格
	private Integer co ;//CO币
	private String prodId ;//规格ID
	private String specId ;//商品ID
	private List<String> imgList;//图片LIST
	private String secSpecName;
	
	private List<ProdSpecInfoBean> specInfolist;
	private List<SpecOptionBean> specOptionBeanList ;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public BigDecimal getPrice() {
		return price;
	}
	public void setPrice(BigDecimal price) {
		this.price = price;
	}
	public Integer getCo() {
		return co;
	}
	public void setCo(Integer co) {
		this.co = co;
	}
	public String getProdId() {
		return prodId;
	}
	public void setProdId(String prodId) {
		this.prodId = prodId;
	}
	public String getSpecId() {
		return specId;
	}
	public void setSpecId(String specId) {
		this.specId = specId;
	}
	public List<String> getImgList() {
		return imgList;
	}
	public void setImgList(List<String> imgList) {
		this.imgList = imgList;
	}
	public String getSecSpecName() {
		return secSpecName;
	}
	public void setSecSpecName(String secSpecName) {
		this.secSpecName = secSpecName;
	}
	public BigDecimal getOriginalPrice() {
		return originalPrice;
	}
	public void setOriginalPrice(BigDecimal originalPrice) {
		this.originalPrice = originalPrice;
	}
	public List<ProdSpecInfoBean> getSpecInfolist() {
		return specInfolist;
	}
	public void setSpecInfolist(List<ProdSpecInfoBean> specInfolist) {
		this.specInfolist = specInfolist;
	}
	public List<SpecOptionBean> getSpecOptionBeanList() {
		return specOptionBeanList;
	}
	public void setSpecOptionBeanList(List<SpecOptionBean> specOptionBeanList) {
		this.specOptionBeanList = specOptionBeanList;
	}
	
	
	
}
