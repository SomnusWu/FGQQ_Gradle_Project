package com.llg.privateproject.entities;

import java.io.Serializable;
import java.util.LinkedHashMap;


public class SpecOptionBean implements Serializable {
	
	private static final long serialVersionUID = -5684613329362440147L;
	private String id ;//名称
	private String name ;//价格
	private LinkedHashMap<String,String> items;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public LinkedHashMap<String,String> getItems() {
		return items;
	}
	public void setItems(LinkedHashMap<String,String> items) {
		this.items = items;
	}
	
	
	
	
}
