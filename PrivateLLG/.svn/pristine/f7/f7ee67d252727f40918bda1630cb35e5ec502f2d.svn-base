package com.bjg.lcc.alipay.pay;import android.util.Log;public class Result {	String resultStatus;	String result;	String memo;	public Result(String rawResult) {//		Log.d("my","rawResult"+rawResult);		try {			String[] resultParams = rawResult.split(";");			for (String resultParam : resultParams) {				if (resultParam.startsWith("resultStatus")) {					resultStatus = gatValue(resultParam, "resultStatus");//					Log.d("my", "resultStatus"+resultStatus);				}				if (resultParam.startsWith("result")) {					result = gatValue(resultParam, "result");//					Log.d("my", "result"+result);//					result="partner=\"2088411879113146\"&seller_id=\"kaijujituan@163.com\"&out_trade_no=\"0105093317-1977\"&subject=\"����ƺ��������\"";//					String[] myresultParams = result.split("&");//					for (String string : myresultParams) {//						if(string.startsWith("out_trade_no"))//						Log.d("my", "trade_no"+getValue(string, "out_trade_no"));//					}									}				if (resultParam.startsWith("memo")) {					memo = gatValue(resultParam, "memo");//					Log.d("my", "memo"+memo);				}//				Log.d("my","rawResult"+resultStatus+result+memo);			}		} catch (Exception e) {			e.printStackTrace();		}	}	@Override	public String toString() {		return "resultStatus={" + resultStatus + "};memo={" + memo				+ "};result={" + result + "}";	}	private String getValue(String content,String key){				String prefix = key + "=\"";		return content.substring(content.indexOf(prefix) + prefix.length(),				content.lastIndexOf("\""));		}	private String gatValue(String content, String key) {		String prefix = key + "={";//		Log.d("my", "content"+content);//		Log.d("my", "key"+key);//		Log.d("my", "prefix"+prefix);//		Log.d("my", "content.indexOf(prefix)"+content.indexOf(prefix) );//		Log.d("my", "content.indexOf(prefix) + prefix.length()"+content.indexOf(prefix) + prefix.length());//		Log.d("my", "prefix"+content.substring(content.indexOf(prefix) + prefix.length(),//				content.lastIndexOf("}")));		return content.substring(content.indexOf(prefix) + prefix.length(),				content.lastIndexOf("}"));	}}